[run]
source = .
omit = 
    */tests/*
    */test_*
    */__pycache__/*
    */migrations/*
    */venv/*
    */env/*
    */.pyenv*/*
    */conftest.py
    */setup.py
    bootstrap_database.sh
    setup_python_venv.sh

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

[html]
directory = coverage_html
title = CRJ Youth E-Library Coverage Report

[xml]
output = coverage.xml 