# Database Configuration
DB_NAME=crjyouthdb

# JWT Configuration
JWT_SECRET_KEY=test-secret-key-for-container
TIMED_SECRET_KEY=test-timed-super-secure-key-for-container

# SMTP Configuration
SMTP_HOST=localhost
SMTP_PORT=587
SMTP_USER=test@example.com
SMTP_PASSWORD=testpass

# Email Addresses
CRJYOUTH_MAIL_NO_REPLY=noreply@example.com

# Logging
# LOG_LEVEL values: DEBUG=10, INFO=20, WARNING=30, ERROR=40, CRITICAL=50
LOG_LEVEL=20 

# Deployment environment
# test -> deploy inside docker container for local testing
# local -> deploy in local environment
# prod -> deploy in prod environment
APP_ENV=test
